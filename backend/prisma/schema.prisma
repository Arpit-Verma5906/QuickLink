datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Link {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  url        String
  shortCode  String   @unique
  createdAt  DateTime
  expiryDate DateTime
}